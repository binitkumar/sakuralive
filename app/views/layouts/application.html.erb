    <section id="contact">
        <div class="container">
            <div class="box last">
                <div class="row">
                    <div class="col-sm-6">
                        <h1>Contact Form</h1>
                        <p>Just drop your contact details and query here. We shall be back to you ....</p>
                        <div class="status alert alert-success" style="display: none"></div>
                        <%= form_for ContactForm.new, id: "main-contact-form", class: "contact-form" do |f| %>
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <input name="contact_form[name]" type="text" class="form-control" required="required" placeholder="Name">
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <input name="contact_form[email]" type="text" class="form-control" required="required" placeholder="Email address">
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <textarea name="contact_form[message]" id="message" required="required" class="form-control" rows="8" placeholder="Message"></textarea>
                                    </div>
                                    <div class="form-group">
                                        <button type="submit" class="btn btn-danger btn-lg">Send Message</button>
                                    </div>
                                </div>
                            </div>
                        <% end %>
                    </div><!--/.col-sm-6-->
